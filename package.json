{
  "name": "nextjs-monorepo-starter",
  "private": true,
  "version": "1.0.0",
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "cypress": "^13.5.1",
    "dotenv-cli": "latest",
    "eslint": "^8.54.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prettier-config-custom": "*",
    "turbo": "latest",
    "typescript": "^5.2.2"
  }
}
